{
	"expo": {
		"name": "Hairsap",
		"slug": "hairsap",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "hairsap",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.reotech.Hairsap",
			"buildNumber": "1.5.8",
			"googleServicesFile": "./GoogleService-Info.plist",
			"infoPlist": {
				"NSCameraUsageDescription": "Hairsap needs access to your camera to capture your face for verification and security reasons during delivery of service.",
				"NSPhotoLibraryUsageDescription": "Hairsap needs permission to access your photo library to upload hairstyle image, set custom avatar and upload videos.",
				"NSLocationWhenInUseUsageDescription": "Hairsap needs users location for connecting clients to the nearest beauty pro for quicker delivery of services",
				"NSLocationAlwaysUsageDescription": "Hairsap needs users location for connecting clients to the nearest beauty pro for quicker delivery of services",
				"UIBackgroundModes": ["remote-notification"]
			}
		},
		"android": {
			"permissions": [
				"CAMERA",
				"ACCESS_FINE_LOCATION",
				"ACCESS_COARSE_LOCATION",
				"CAMERA_ROLL",
				"RECEIVE_BOOT_COMPLETED",
				"READ_EXTERNAL_STORAGE",
				"android.permission.CAMERA",
				"android.permission.ACCESS_COARSE_LOCATION",
				"android.permission.ACCESS_FINE_LOCATION",
				"android.permission.RECORD_AUDIO"
			],
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"softwareKeyboardLayoutMode": "resize",
			"package": "com.igoni.Hairsap",
			"googleServicesFile": "./google-services.json",
			"versionCode": 10
		},
		"androidNavigationBar": {
			"barStyle": "light-content",
			"backgroundColor": "#000000"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash.png",
					"resizeMode": "contain",
					"backgroundColor": "#000000"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "71d45a98-6411-4e92-b7bf-adcc13f4ee66"
			}
		},
		"description": "App for connecting users to hairstylists",
		"owner": "hairsap"
	}
}
